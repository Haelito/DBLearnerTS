			"properties": {
				"activities": [
					{
						"name": "PL_DS_AMS3_Movies_To_DS_ADLS_Movies",
						"description": "Transporta datos de películas en formato csv desde Amazon S3 hacia Azure Data Lake Storage en formato parquet",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "0.12:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
